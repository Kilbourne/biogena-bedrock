{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "A modern WordPress stack",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock", "roots", "wordpress", "stack", "composer", "vagrant", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.3.1",
    "wpackagist-plugin/advanced-custom-fields": "4.4.3",
    "roots/soil": "3.6.0",
    "wpackagist-plugin/admin-post-navigation": "1.9.2",
    "lamosty/bedrock-plugin-control": "^1.0",    
    "wpackagist-plugin/post-types-order": "1.8.5",
    "wpackagist-plugin/menu-image": "2.6.3",
    "wpackagist-plugin/posts-to-posts": "1.6.5",
    "wpackagist-plugin/post-type-switcher": "1.5",
    "wpackagist-plugin/custom-post-type-ui": "1.1.2",
    "wpackagist-plugin/responsive-menu": "2.8.5",
    "wpackagist-plugin/updraftplus": "1.11.18",
    "wpackagist-plugin/transients-manager": "1.7.2",    
    "wpackagist-plugin/ricg-responsive-images": "3.1.1",
    "wpackagist-plugin/duplicate-post": "2.6",
    "wpackagist-plugin/svg-support": "2.2.2",
    "wpackagist-plugin/wordpress-importer": "0.6.1",    
    "kilbourne/biogena-data": "2.2.10",
    "wpackagist-plugin/enable-media-replace": "3.0.4",
    "wpackagist-plugin/members": "1.1.0",
    "wpackagist-plugin/toggle-wpautop": "1.2.0",
    "wpackagist-plugin/disable-comments": "1.4",
    "wpackagist-plugin/favicon-by-realfavicongenerator": "1.3.2",
    "sunra/php-simple-html-dom-parser": "v1.5.0",
    "wpackagist-plugin/asset-queue-manager": "1.0.1",
    "wpackagist-plugin/ewww-image-optimizer": "2.5.9"
  },
  "require-dev": {
    "wpackagist-plugin/p3-profiler": "1.5.3.9",
    "wpackagist-plugin/show-current-template": "0.2.2",
    "wpackagist-plugin/query-monitor": "2.8.1",
    "wpackagist-plugin/debug-bar": "0.8.2",
    "wpackagist-plugin/plugins-garbage-collector": "0.10.1",
    "wpackagist-plugin/wp-sweep": "1.0.6",
    "wpackagist-plugin/options-inspector": "2.1.1",
    "wpackagist-plugin/rewrite-rules-inspector": "1.2.1",
    "wpackagist-plugin/gtmetrix-for-wordpress": "0.4.1",
    "wpackagist-plugin/google-pagespeed-insights": "1.0.6",
    "wpackagist-plugin/validated": "2.1.1"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  }
}
